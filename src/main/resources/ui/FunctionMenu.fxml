<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.*?>
<fx:root type="StackPane" xmlns:fx="http://javafx.com/fxml/1">
    <BorderPane styleClass="menu">

        <padding>
            <Insets top="3" right="3" bottom="3" left="3"/>
        </padding>
        <top>
            <FlowPane fx:id="searchSpace" prefWrapLength="10" vgap="50" hgap="10"/>
        </top>
        <center>
            <TilePane fx:id="utilSpace" alignment="CENTER" prefColumns="2" hgap="5" vgap="5"/>
        </center>
        <bottom>
            <VBox fx:id="categorySpace" prefWidth="15"/>
        </bottom>
    </BorderPane>

</fx:root>
