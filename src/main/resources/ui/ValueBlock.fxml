<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<?import java.lang.String?>
<?import java.net.URL?>
<fx:root type="StackPane" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1">
    <VBox prefWidth="150" minWidth="150">
        <stylesheets>
            <URL value="@layout.css"/>
            <URL value="@colours.css"/>
        </stylesheets>
        <styleClass>
            <String fx:value="value"/>
            <String fx:value="block"/>
        </styleClass>
        <HBox alignment="TOP_CENTER" minHeight="20.0" styleClass="header">
            <Label text="Constant" textFill="WHITE"/>
        </HBox>
        <VBox styleClass="blockcontent">
            <BorderPane>
                <left>
                    <StackPane alignment="BOTTOM_LEFT">
                        <TextField fx:id="numberInputTextField" maxHeight="27.0" maxWidth="-Infinity" minHeight="27" minWidth="-Infinity"
                                   prefHeight="27.0" prefWidth="120.0" styleClass="inputValue"/>
                        <padding>
                            <Insets bottom="3.0" left="3.0" top="3.0"/>
                        </padding>
                    </StackPane>
                </left>
                <right>
                    <StackPane fx:id="outputSpace" minHeight="20.0" prefHeight="20.0"/>
                </right>
            </BorderPane>
        </VBox>
    </VBox>
</fx:root>
